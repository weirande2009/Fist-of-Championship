{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ 生成活动文件",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "${fileDirname}/ClientPool.h",
                "${fileDirname}/ClientPool.cpp",
                "${fileDirname}/Database.h",
                "${fileDirname}/Database.cpp",
                "${fileDirname}/Lobby.h",
                "${fileDirname}/Lobby.cpp",
                "${fileDirname}/MainClient.h",
                "${fileDirname}/MainClient.cpp",
                "${fileDirname}/MainServer.h",
                "${fileDirname}/MainServer.cpp",
                "${fileDirname}/MainServerController.h",
                "${fileDirname}/MainServerController.cpp",
                "${fileDirname}/Player.h",
                "${fileDirname}/Player.cpp",
                "${fileDirname}/Room.h",
                "${fileDirname}/Room.cpp",
                "${fileDirname}/Protocols.h",
                "${fileDirname}/Tables.h",
                "${fileDirname}/../common/common.h",
                "${fileDirname}/../common/Server.h",
                "${fileDirname}/../common/Client.h",
                "${fileDirname}/../common/my_network.h",
                "${fileDirname}/../Protobuf/ClientToServer.pb.h",
                "${fileDirname}/../Protobuf/ClientToServer.pb.cc",
                "${fileDirname}/../Protobuf/ServerToClient.pb.h",
                "${fileDirname}/../Protobuf/ServerToClient.pb.cc",
                "-lprotobuf",
                "-lmysqlclient",
                "-lpthread",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}